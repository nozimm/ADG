{
    "name": "ADG",
    "version": "1.0",
    "description": "no fingerprints",
    "permissions": ["storage", "webNavigation", "webRequest", "webRequestBlocking", "<all_urls>"],
    "background": {
        "scripts": ["inject/agents.js", "background.js"],
        "persistent": true
    },
    "manifest_version": 2,
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    }


}